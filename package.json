{
  "name": "tron3",
  "version": "2.0.0",
  "description": "A dead-simple voice channel notifications bot for your Discord server",
  "private": true,
  "type": "module",
  "main": "build/index.js",
  "engines": {
    "node": ">=16.9"
  },
  "repository": {
    "url": "https://github.com/zakuciael/tron3.git",
    "type": "git"
  },
  "author": "Krzysztof Saczuk <zakku@zakku.eu>",
  "license": "MIT",
  "keywords": [
    "bot",
    "discord",
    "discord-bot",
    "tron",
    "tron3",
    "notifications",
    "voice-channel",
    "voice-notifications",
    "channel-notifications"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "husky:init": "husky install",
    "lint": "xo --fix",
    "clean": "rimraf build/",
    "build": "swc src/ -d build/",
    "start": "node build/index.js",
    "dev": "run-s clean build start"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "discord-api-types": "^0.37.1",
    "eslint": "^8.20.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-config-xo-typescript": "^0.52.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "rimraf": "^3.0.2",
    "tsc-files": "^1.1.3",
    "typescript": "^4.7.4",
    "xo": "^0.51.0"
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "@swc/helpers": "^0.4.6",
    "@zakku/winston-logs": "^1.3.1",
    "discord.js": "^14.1.2",
    "dotenv": "^16.0.1",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.8.1"
  }
}
