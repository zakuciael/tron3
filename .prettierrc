{
  "printWidth": 110,
  "tabWidth": 4,
  "endOfLine": "lf",
  "semi": true,
  "useTabs": false,
  "singleQuote": false,
  "bracketSpacing": true,
  "trailingComma": "none",
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "overrides": [
    {
      "files": [
        "*.json",
        ".prettierrc",
        ".commitlintrc",
        ".eslintrc",
        ".xo-config",
        ".lintstagedrc",
        ".swcrc"
      ],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
